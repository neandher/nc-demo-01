<!DOCTYPE html>
<html lang="en">
<!-- begin::Head -->
<head>
    <meta charset="utf-8"/>
    <title>
        {{ title }} | {% block title %}{% endblock %}
    </title>
    <meta name="description" content="Azap Admin">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!--begin::Web font -->
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.16/webfont.js"></script>
    <script>
        WebFont.load({
            google: {"families":["Poppins:300,400,500,600,700","Roboto:300,400,500,600,700"]},
            active: function() {
                sessionStorage.fonts = true;
            }
        });
    </script>
    <!--end::Web font -->
    {% block stylesheets %}
        {% embed 'admin/_partials/_block_stylesheets.html.twig' %}{% endembed %}
    {% endblock %}
</head>
<!-- end::Head -->

{% block body %}
    <body  class="m-page--fluid m--skin- m-content--skin-light2 m-header--fixed m-header--fixed-mobile m-aside-left--enabled m-aside-left--skin-dark m-aside-left--offcanvas m-footer--push m-aside--offcanvas-default"  >
    <!-- begin:: Page -->
    <div class="m-grid m-grid--hor m-grid--root m-page">
        <!-- BEGIN: Header -->
        {% include 'admin/_partials/_header.html.twig' %}
        <!-- END: Header -->
        <!-- begin::Body -->
        <div class="m-grid__item m-grid__item--fluid m-grid m-grid--ver-desktop m-grid--desktop m-body">
            <!-- BEGIN: Left Aside -->
            <button class="m-aside-left-close  m-aside-left-close--skin-dark " id="m_aside_left_close_btn">
                <i class="la la-close"></i>
            </button>
            <div id="m_aside_left" class="m-grid__item	m-aside-left  m-aside-left--skin-dark ">
                <!-- BEGIN: Aside Menu -->
                {% include 'admin/_partials/_aside_menu.html.twig' %}
                <!-- END: Aside Menu -->
            </div>
            <!-- END: Left Aside -->
            <div class="m-grid__item m-grid__item--fluid m-wrapper">
                <!-- BEGIN: Subheader -->
                {% block page_subheader %}{% endblock %}
                <!-- END: Subheader -->
                <div class="m-content">
                    {% block page_content %}{% endblock %}
                </div>
            </div>
        </div>
        {% block footer %}
            {% embed 'admin/_partials/_footer.html.twig' %}{% endembed %}
        {% endblock %}
    </div>
    {% block javascripts %}
        {% embed 'admin/_partials/_block_javascripts.html.twig' %}{% endembed %}
    {% endblock %}
    </body>
{% endblock %}
</html>