        <td width="358" valign="middle" height="54" style="font-family: sans-serif;font-size: 13px;text-align: left; padding-left: 10px;">{{ schedule.customer.customerAddresses.first.address.city }}</td>
    </tr>
    <tr style="background-color:#fff;">
        <td width="108" valign="middle" height="54" style="font-family: sans-serif;font-size: 13px;text-align: left; padding-left: 10px;">{{ 'address.fields.state'|trans }}:</td>
        <td width="358" valign="middle" height="54" style="font-family: sans-serif;font-size: 13px;text-align: left; padding-left: 10px;">{{ schedule.customer.customerAddresses.first.address.state }}</td>
    </tr>
    <tr style="background-color:#f4f5f8;">
        <td width="108" valign="middle" height="54" style="font-family: sans-serif;font-size: 13px;text-align: left; padding-left: 10px;">{{ 'address.fields.zipCode'|trans }}:</td>
        <td width="358" valign="middle" height="54" style="font-family: sans-serif;font-size: 13px;text-align: left; padding-left: 10px;">{{ schedule.customer.customerAddresses.first.address.zipCode }}</td>
    </tr>
    <tr style="background-color:#fff;">
        <td width="108" valign="middle" height="54" style="font-family: sans-serif;font-size: 13px;text-align: left; padding-left: 10px;">{{ 'schedule.fields.instructions'|trans }}:</td>
        <td width="358" valign="middle" height="54" style="font-family: sans-serif;font-size: 13px;text-align: left; padding-left: 10px;">{{ schedule.instructions }}</td>
    </tr>
    <tr>
        <td colspan="2" height="20" style="font-size:0;line-height:0;">&nbsp;</td>
    </tr>
    </tbody>
</table>