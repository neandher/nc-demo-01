{% if prototype is defined %}
<div class='form-group m-form__group row'>
    <div class='col-lg-12'>
        {{ form_label(item.address, {}, {'label_attr': {'class': 'col-form-label'}})|e('html_attr') }}
        {{ form_widget(item.address, {
            'attr': {
                'class' : 'm-input',
            }
        })|e('html_attr') }}
    </div>
</div>
<div class='form-group m-form__group row'>
    <div class='col-lg-6'>
        {{ form_label(item.city, {}, {'label_attr': {'class': 'col-form-label'}})|e('html_attr') }}
        {{ form_widget(item.city, {
            'attr': {
                'class' : 'm-input',
            }
        })|e('html_attr') }}
    </div>
    <div class='col-lg-6'>
        {{ form_label(item.state, {}, {'label_attr': {'class': 'col-form-label'}})|e('html_attr') }}
        {{ form_widget(item.state, {
            'attr': {
                'class' : 'm-input',
            }
        })|e('html_attr') }}
    </div>
</div>
<div class='form-group m-form__group row'>
    <div class='col-lg-6'>
        {{ form_label(item.zipCode, {}, {'label_attr': {'class': 'col-form-label'}})|e('html_attr') }}
        {{ form_widget(item.zipCode, {
            'attr': {
                'class' : 'm-input',
                'readonly': true
            }
        })|e('html_attr') }}
    </div>
</div>
{% endif %}
