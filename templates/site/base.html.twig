<!DOCTYPE html>
<html lang="en" >
<!-- begin::Head -->
<head>
    <meta charset="utf-8" />
    <title>{{ title_site }} | {% block title %}Welcome{% endblock %}</title>
    <meta name="description" content="Demo 01 System">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!--begin::Web font -->
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.16/webfont.js"></script>
    <script>
        WebFont.load({
            google: {"families":["Poppins:300,400,500,600,700","Roboto:300,400,500,600,700"]},
            active: function() {
                sessionStorage.fonts = true;
            }
        });
    </script>
    <!--end::Web font -->
    <!--begin::Base Styles -->
    {% block stylesheets %}
        {% embed 'site/_partials/_block_stylesheets.html.twig' %}{% endembed %}
    {% endblock %}
    <!--end::Base Styles -->
</head>

{% block body %}
    <body class="m-page--wide m-header--fixed m-header--fixed-mobile m-footer--push m-aside--offcanvas-default">

    <!-- Load Facebook SDK for JavaScript -->
    <div id="fb-root"></div>

    <!-- Your customer chat code -->
    <div class="fb-customerchat"
         attribution=setup_tool
         page_id="355383231569532">
    </div>

    <!-- begin:: Page -->
    <div class="m-grid m-grid--hor m-grid--root m-page">
        <!-- BEGIN: Header -->
        {% include 'site/_partials/_header.html.twig' %}
        <!-- END: Header -->
        <!-- begin::Body -->
        <div class="m-grid__item m-grid__item--fluid  m-grid m-grid--ver-desktop m-grid--desktop 	m-container m-container--responsive m-container--xxl m-page__container m-body">

            <div class="m-grid__item m-grid__item--fluid m-wrapper">
                <div class="m-content">
                    {% block page_content %}{% endblock %}
                </div>
            </div>
        </div>
        {% block footer %}
            {% embed 'site/_partials/footer.html.twig' %}{% endembed %}
        {% endblock %}
    </div>
    {% block javascripts %}
        {% embed 'site/_partials/_block_javascripts.html.twig' %}{% endembed %}
    {% endblock %}
    </body>
{% endblock %}

